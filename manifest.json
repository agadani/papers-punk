{
    "name": "Papers, punk",
    "description": "Randomly chooses a file from a specified directory and displays it. ",
    "version": "0.0.0.1",
    "manifest_version": 2,
    "permissions": ["file:///*", "storage"],
    "options_page": "options.html",
    "icons": { "16": "pp_icon_16.png",
             "32": "pp_icon_32.png",
             "48": "pp_icon_48.png",
             "128": "pp_icon_128.png" },
    "chrome_url_overrides" : {
        "newtab": "newtab.html"
    }
 }
